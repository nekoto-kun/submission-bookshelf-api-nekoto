{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:node/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "no-process-exit": "off"
  },
  "node/no-unsupported-features/es-syntax": [
    "error",
    {
      "version": ">=8.0.0",
      "ignores": []
    }
  ]
}